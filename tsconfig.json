{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"importsNotUsedAsValues": "remove",
		"jsx": "react",
		"lib": ["ESNext", "DOM"],
		"noEmit": true,
		"noImplicitAny": false,
		"paths": { "@site/*": ["./*"] },
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"esModuleInterop": true
	},
	"include": ["src", "docs"],
	"exclude": []
}
